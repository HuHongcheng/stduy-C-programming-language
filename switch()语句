
#define _CRT_SECURE_NO_WARNINGS 1

//输入数字 输出对应的日期!
#include<stdio.h>

int main()
{
	int day = 0;
	printf("请输入星期:");
	scanf("%d", &day);
	switch (day)
	{
	case 1:
	case 2:
	case 3:
	case 4:
	case 5:
		printf("工作日");
		break;
	case 6:
	case 7:
		printf("休息日");
		break;
	default:
		printf("请输入正确的日期");
		break;
	}
	return 0;
}
