//习题：打印1-10数字

#include<stdio.h>

//打印1-10
int main()
{
	int i = 1;
	do
	{
		printf("%d ", i);
		i++;

	} 
	while (i<11);

	return 0;
 }



//注意i++的位置 
//当i++在continue后方时:
#include<stdio.h>

//打印1-10
int main()
{
	int i = 1;
	do
	{
		if (i = 5)
			continue;
		printf("%d ", i);
		i++;

	} 
	while (i<11);

	return 0;
 }




//当i+++在continue的前方:
#include<stdio.h>

//打印1-10
int main()
{
	int i = 0;
	do
	{
		i++;

		if (i == 5)
			continue;
		printf("%d ", i);
	} 
	while (i<11);

	return 0;
 }
