
#include<stdio.h>
//规定a最大 b次之 c最小
int main()
{
	int a = 0;
	int b = 0;
	int c = 0;
	printf("请输入3个数");
	scanf("%d%d%d", &a, &b, &c);
	if (c > b)
	{
		int y = b;
		b = c;
		c = y;
	}
	if (b > a)
	{
		int x = a;
		a = b;
		b = x;
	}
	if (c>a)
	{
		int h = a;
		a = c;
		c = h;
	}
	printf("%d %d %d", a, b, c);
	return 0;

}
